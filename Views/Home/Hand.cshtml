@model HandOverviewViewModel

@{
    ViewData["Title"] = "Win Percentage";
}


@Model.Hand


@foreach (HandHistory hh in Model.Hands)
{
    <p>
    @foreach(string action in hh.Actions)
    {
        <span>@action</span><br/>
    }

    </p>
    <p>

    @foreach(string line in hh.RawHand)
    {
        <span>@line</span><br/>
    }

    </p>
}